public async searchCases(filter: {
  text?: string;
  fromDate?: string;
}): Promise<ICaseRow[]> {

  let filters: string[] = [];

  if (filter.text) {
    filters.push(
      `(CivilId eq '${filter.text}' or CaseId eq '${filter.text}')`
    );
  }

  if (filter.fromDate) {
    filters.push(`Created ge datetime'${filter.fromDate}T00:00:00Z'`);
  }

  const filterQuery = filters.length > 0
    ? "&$filter=" + filters.join(" and ")
    : "";

  const url =
    `${this.siteUrl}/_api/web/lists/getbytitle('DisputeCases')/items` +
    `?$select=Id,CaseId,CivilId,Status,Created` +
    `&$orderby=Created desc` +
    filterQuery;

  return this.spHttpClient.get(url, SPHttpClient.configurations.v1)
    .then(r => r.json())
    .then(d => d.value);
}