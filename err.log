/* Submenu active item */
.submenu li.sub-active {
  background: #edf0f3;
  color: #c00000;
  font-weight: 600;
  position: relative;
}

/* Small red indicator for submenu (optional) */
.submenu li.sub-active::before {
  content: "";
  position: absolute;
  left: 18px;
  top: 10px;
  bottom: 10px;
  width: 2px;
  background: #c00000;
  border-radius: 2px;
}