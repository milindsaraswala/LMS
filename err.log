private renderDashboardGrid(): void {

  const context = this.context;

  // Prevent duplicate instance
  const existingInstance = $("#dashboardGrid").dxDataGrid("instance");
  if (existingInstance) {
    existingInstance.dispose();
    $("#dashboardGrid").empty();
  }

  $("#dashboardGrid").dxDataGrid({
    dataSource: this.getCoursesItems(),   // reuse your existing method
    paging: {
      pageSize: 10
    },
    pager: {
      showPageSizeSelector: true,
      allowedPageSizes: [10, 20, 50],
      showInfo: true
    },
    searchPanel: {
      visible: true,
      width: 250,
      placeholder: "Search..."
    },
    showBorders: false,
    columnAutoWidth: true,
    rowAlternationEnabled: true,
    hoverStateEnabled: true,
    columns: [
      {
        dataField: "programName",
        caption: "Program Name",
        width: 250
      },
      {
        dataField: "trainingCategory",
        caption: "Category",
        width: 200
      },
      {
        dataField: "startDate",
        caption: "Start Date",
        dataType: "date",
        format: "dd-MMM-yyyy"
      },
      {
        dataField: "endDate",
        caption: "End Date",
        dataType: "date",
        format: "dd-MMM-yyyy"
      },
      {
        dataField: "venue",
        caption: "Venue"
      }
    ]
  });

}

.bbs-grid-wrapper {
  background: #ffffff;
  border: 1px solid #dcdcdc;
  padding: 20px;
  box-shadow: 0 2px 6px rgba(0,0,0,0.04);
}

private getCoursesItems = () => {
  return new DevExpress.data.CustomStore({
    loadMode: "raw",
    load: async () => {
      return await this.getData(0);  // dashboard = upcoming
    }
  });
}

