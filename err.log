public async deleteDisputeItem(itemId: number): Promise<void> {
  if (!itemId) {
    throw new Error("Invalid dispute item id");
  }

  const url =
    `${this.siteUrl}/_api/web/lists/getbytitle('${this.listTitle}')/items(${itemId})`;

  const response = await this.spHttpClient.fetch(url, SPHttpClient.configurations.v1, {
    method: "POST",
    headers: {
      "IF-MATCH": "*",
      "X-HTTP-Method": "DELETE",
      "Accept": "application/json;odata=nometadata"
    }
  });

  if (!response.ok) {
    const text = await response.text();
    throw new Error("Failed to delete dispute item: " + text);
  }
}