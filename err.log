/* Content wrapper */
.bbs-content {
  padding: 30px 40px;
  background: #edf0f3;
}

/* White card */
.bbs-card {
  background: #ffffff;
  border: 1px solid #dcdcdc;
  box-shadow: 0 2px 6px rgba(0,0,0,0.05);
  padding: 40px;
}

/* Flex layout */
.bbs-card-inner {
  display: flex;
  align-items: center;
  justify-content: space-between;
  gap: 40px;
}

/* Left content */
.bbs-card-left {
  flex: 1;
}

.bbs-title {
  font-size: 26px;
  font-weight: 600;
  margin-bottom: 20px;
  color: #333;
}

.bbs-card-left p {
  font-size: 14px;
  line-height: 1.6;
  color: #555;
  margin-bottom: 15px;
}

/* Button */
.bbs-primary-btn {
  margin-top: 15px;
  background: #c00000;
  color: #ffffff;
  border: none;
  padding: 10px 25px;
  font-size: 14px;
  cursor: pointer;
  border-radius: 3px;
}

.bbs-primary-btn:hover {
  background: #a00000;
}

/* Right image */
.bbs-card-right {
  flex: 1;
  text-align: right;
}

.bbs-card-right img {
  max-width: 100%;
  height: auto;
}