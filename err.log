.bbs-left-panel li {
  display: flex;
  align-items: center;
  gap: 10px;
  padding: 10px 20px 10px 25px;
  font-size: 14px;
  color: #4a4a4a;
}

.bbs-icon {
  width: 18px;
  display: flex;
  align-items: center;
}

.bbs-left-panel li.active svg {
  stroke: #c00000;
  fill: #c00000;
}


<ul id="bbsMenu">
  <li data-view="about" class="active">
    <span class="bbs-icon">
      <svg viewBox="0 0 24 24" width="16" height="16">
        <circle cx="12" cy="12" r="10" stroke="#888" fill="none" stroke-width="2"/>
      </svg>
    </span>
    About
  </li>

  <li data-view="dashboard">
    <span class="bbs-icon">
      <svg viewBox="0 0 24 24" width="16" height="16">
        <rect x="3" y="3" width="7" height="7" fill="#888"/>
        <rect x="14" y="3" width="7" height="7" fill="#888"/>
        <rect x="3" y="14" width="7" height="7" fill="#888"/>
        <rect x="14" y="14" width="7" height="7" fill="#888"/>
      </svg>
    </span>
    Dashboard
  </li>
</ul>
