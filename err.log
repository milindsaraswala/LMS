  public async updateRefundStatus(itemId: number, status: string): Promise<void> {
    const url =
      `${this.siteUrl}/_api/web/lists/getbytitle('${encodeURIComponent(this.listTitle)}')/items(${itemId})`;

    const body = {
      __metadata: { type: "SP.Data.FlowardDisputesListItem" },
      RefundStatus: status
    };

    const resp: SPHttpClientResponse = await this.spHttpClient.post(
      url,
      SPHttpClient.configurations.v1,
      {
        headers: {
          "Accept": "application/json;odata=verbose",
          "Content-Type": "application/json;odata=verbose",
          "IF-MATCH": "*",
          "X-HTTP-Method": "MERGE"
        },
        body: JSON.stringify(body)
      }
    );

    if (!resp.ok) {
      const t = await resp.text();
      throw new Error("Failed to update refund status: " + t);
    }
  }
